<?xml version="1.0" encoding="UTF-8"?>
<Patient xmlns="http://hl7.org/fhir">    
    <id value="addressStructuredInternationalAddressexample3"/>   
    <text>
        <status value="additional"/>
        <div xmlns="http://www.w3.org/1999/xhtml">            
            <p>This sample XML is an example for the use case of fully structured international address.</p>
            <p>This sample XML contains Patient as a parent element since the address complex datatype does not contain neither .xsd nor .sch files to validate the structure of the XML.</p>            
        </div>
    </text>    
    <address>        
        <use value="work" />        
        <type value = "postal"/>        
        <text value="21A Main Street, Auckland Central, Auckland, New Zealand"/>   
        <line value="RG 5th floor" />        
        <city value="Auckland" />             
        <state value="North Island" />        
        <postalCode value="5000" />        
        <country value="NZ"/>        
        <period>
            <start value="2013-06-08T10:57:34+01:00"/> 
        </period>
        
    </address>
    
</Patient>